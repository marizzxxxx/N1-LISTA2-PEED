#questao 5

grafo = {}

ver1 = input("Digite os vÃ©rtices separados por espaÃ§o: ").split()

for ver2 in ver1:
    grafo[ver2] = []

a = input("Digite as arestas separadas (ex: a b, c d): ").split(",")

for a in a:
    v1, v2 = a.strip().split()
    grafo[v1].append(v2)
    grafo[v2].append(v1)